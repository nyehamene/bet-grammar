{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "betwork",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_stmt"
      }
    },
    "_stmts": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_stmt"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_separator"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_stmt"
                  }
                ]
              }
            }
          ]
        }
      ]
    },
    "_stmt": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "dir_statement"
            },
            {
              "type": "SYMBOL",
              "name": "var_statement"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_separator"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "dir_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@dir"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "var_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "::"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "string_line_group"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "member_access"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number_decimal"
        },
        {
          "type": "SYMBOL",
          "name": "number_hexadecimal"
        },
        {
          "type": "SYMBOL",
          "name": "number_binary"
        }
      ]
    },
    "number_decimal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "0"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[1-9]"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "PATTERN",
                  "value": "[0-9]"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "."
                      },
                      {
                        "type": "REPEAT",
                        "content": {
                          "type": "PATTERN",
                          "value": "[0-9]"
                        }
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "number_hexadecimal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "0x"
              },
              {
                "type": "STRING",
                "value": "0X"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "PATTERN",
              "value": "[a-fA-F0-9]"
            }
          }
        ]
      }
    },
    "number_binary": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "0b"
              },
              {
                "type": "STRING",
                "value": "0B"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "0"
                },
                {
                  "type": "STRING",
                  "value": "1"
                }
              ]
            }
          }
        ]
      }
    },
    "member_access": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_string_content"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "string_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\\\"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_string_content"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "string_line_group": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "string_line"
      }
    },
    "_string_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "SYMBOL",
          "name": "string_template_expr"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n\"]"
        }
      ]
    },
    "string_template_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\{"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "escape_char": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "\\\\"
        },
        {
          "type": "STRING",
          "value": "\\\""
        },
        {
          "type": "STRING",
          "value": "\\t"
        },
        {
          "type": "STRING",
          "value": "\\r"
        },
        {
          "type": "STRING",
          "value": "\\s"
        },
        {
          "type": "STRING",
          "value": "\\f"
        },
        {
          "type": "STRING",
          "value": "\\v"
        },
        {
          "type": "STRING",
          "value": "\\n"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_separator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}