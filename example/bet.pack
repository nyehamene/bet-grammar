pkg :: package("github.com/bet/betui")

version("0.12.0")

require("github.com/bet/betutil@v0.10.0")
require("github.com/other/ui@v10.0.0")

export(user: pkg.User) {
	enum(
	    user.Gender,
	    user.Role,
	)

	css(
	   user.Form,
	   user.Table,
    )

	component(
	    user.Form,
	    user.Table,
	)

	document(
		user.Dashboard,
		user.Login
	)
}

export(rpg: pkg.rpg) compose(enum, component, document) {
    rpg.WeaponType
    rpg.Weapon
    rpg.Role
    rpg.Monster
    rpg.Dashboard
    rpg.Inventory

	css (
		rgp.WeaponType,
	)
}

