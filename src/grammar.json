            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "0b"
                  },
                  {
                    "type": "STRING",
                    "value": "0B"
                  }
                ]
              },
              {
                "type": "REPEAT1",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "0"
                    },
                    {
                      "type": "STRING",
                      "value": "1"
                    }
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    "keyword": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "'"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
              },
              {
                "type": "PATTERN",
                "value": "(--)?[a-zA-Z_-][a-zA-Z0-9_-]*"
              }
            ]
          }
        ]
      }
    },
    "_separator": {
      "type": "TOKEN",
