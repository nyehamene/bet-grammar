            "type": "STRING",
            "value": "\u0000"
          }
        ]
      }
    },
    "comment_line": {
      "type": "PREC",
      "value": 0,
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": "[^\\n]*"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\n"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      }
    },
    "comment_line_group": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "comment_line"
          },
          "named": true,
          "value": "comment"
