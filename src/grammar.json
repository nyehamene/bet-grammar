                "value": "n"
              }
            ]
          }
        ]
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z_][a-zA-Z0-9_]*"
      }
    },
    "identifier_dot": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\.[a-zA-Z_-][a-zA-Z0-9_-]*"
      }
    },
    "identifier_dash": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "(--)?[a-zA-Z_-][a-zA-Z0-9_-]*"
      }
    },
    "identifier_builtin": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@[a-zA-Z_][a-zA-Z0-9_-]*"
      }
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "member_access"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
