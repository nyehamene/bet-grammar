        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_string_content"
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_string_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "escape_char"
        },
        {
          "type": "SYMBOL",
          "name": "string_template_expr"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[^\"\\\\\\n]+?"
          }
        }
      ]
    },
    "string_line_group": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "string_line"
        }
      }
    },
    "string_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\\\"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_string_line_content"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "\\n"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_string_line_content": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "escape_char"
          },
          {
            "type": "SYMBOL",
            "name": "string_template_expr"
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[^\\\\\\n]+?"
            }
          }
        ]
      }
    },
    "string_template_expr": {
      "type": "SYMBOL",
      "name": "template_expression"
    },
    "template_expression": {
      "type": "SEQ",
      "members": [
