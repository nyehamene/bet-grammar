        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "escape_char": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\\"
